{
  "name": "raichuru-belku-newspaper",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "./node_modules/.bin/vite",
    "build": "./node_modules/.bin/vite build",
    "preview": "./node_modules/.bin/vite preview",
    "test-backend": "node test-backend.js",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "start:all": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "setup": "npm install && cd backend && npm install",
    "fix": "npm run setup && echo Setup complete!"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.22",
    "pdfjs-dist": "^3.11.174",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwindcss": "^3.4.0",
    "vite": "^4.5.14"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1"
  }
}
